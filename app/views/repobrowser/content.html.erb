<ul class="jqueryFileTree" style="display: none;">
  <% @entries[0].each do |dir| %>
    <li class="directory collapsed">
      <a href="#" rel="<%= @parent + dir %>/"><%= dir %></a>
    </li>
  <% end %>
  <% @entries[1].each do |file| %>
    <li class="file ext_<%= File.extname(file)[1..-1] %>">
      <a href="#" rel="<%= @parent + file %>"><%= file %></a>
    </li>
  <% end %>
</ul>
